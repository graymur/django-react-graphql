{
  "name": "django-react-graphql",
  "version": "1.0.0",
  "description": "django-react-graphql",
  "main": "./src/server/server-prod-local.js",
  "scripts": {
    "coverage": "cross-env NODE_ENV=test NODE_PATH=./src jest --coverage",
    "test": "cross-env NODE_ENV=test NODE_PATH=./src jest --runInBand",
    "test:watch": "npm run test -- --watch",
    "start": "cross-env NODE_ENV=production NODE_PATH=./src babel-node ./src/server/server-prod-local.js",
    "mon": "nodemon --ignore ./src/shared --ignore ./src/client --exec npm start",
    "dev": "cross-env NODE_ENV=development NODE_PATH=./src babel-node ./src/server/server-dev.js",
    "devmon": "nodemon -e js,ejs,graphql --ignore ./src/client --exec yarn dev",
    "build": "yarn dll:prod && cross-env SPA_API_BASE_URL=/app-test-api SPA_BUILD_PATH=../../app/static/spa SPA_ASSETS_PATH=/static/spa/ SPA_URL_BASE=/app NODE_ENV=production NODE_PATH=./src yarn webpack --optimize-minimize --color --progress --config=webpack/webpack.config.prod.js",
    "build:staging": "yarn dll:prod && cross-env SPA_API_BASE_URL=http://localhost:3333 SPA_BUILD_PATH=../../app/static/spa SPA_ASSETS_PATH=/static/spa/ SPA_URL_BASE=/app IMG_URL_BASE=/static/img NODE_ENV=production NODE_PATH=./src yarn webpack --optimize-minimize --color --progress --config=webpack/webpack.config.prod.js",
    "build:local": "yarn dll:prod && cross-env SPA_API_BASE_URL=http://localhost:3333 WEBAPP_API_BASE_URL=/api SPA_BUILD_PATH=../build SPA_ASSETS_PATH=/ IMG_URL_BASE=/ NODE_ENV=production NODE_PATH=./src yarn webpack --optimize-minimize --color --progress --config=webpack/webpack.config.prod.js",
    "dll:dev": "cross-env NODE_ENV=development NODE_PATH=./src yarn webpack --color --progress --config=webpack/webpack.config.dll.js",
    "dll:prod": "cross-env NODE_ENV=production NODE_PATH=./src yarn webpack --color --progress --config=webpack/webpack.config.dll.js",
    "dll": "yarn dll:dev && yarn dll:prod",
    "postinstall": "yarn dll:dev",
    "lint": "prettier ./src/**/*.js",
    "lint:fix": "prettier --write \"{src,webpack}/**/*.js\"",
    "webpack": "babel-node node_modules/webpack/bin/webpack.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-client-preset": "^1.0.6",
    "apollo-server-express": "^1.3.6",
    "bluebird": "^3.5.2",
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "luxon": "^1.3.0",
    "merge-graphql-schemas": "^1.5.1",
    "muicss": "^0.9.39",
    "prop-types": "^15.6.1",
    "query-string": "^5.0.1",
    "react": "^16.4.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-apollo": "^2.1.5",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.4.2",
    "react-draggable": "^3.0.5",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.4.0",
    "react-monaco-editor": "^0.14.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.8",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^4.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^7.0.0",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "express": "^4.16.3",
    "express-delay": "^0.2.0",
    "fetch": "^1.1.0",
    "file-loader": "^1.1.11",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.1.1",
    "graphql-type-json": "^0.2.1",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^3.0.0",
    "jest": "^22.0.3",
    "merge-deep": "^3.0.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-noop": "^1.0.0",
    "node-sass": "^4.9.3",
    "opener": "^1.4.2",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.14.3",
    "react-hot-loader": "^4.3.11",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "testURL": "http://localhost/",
    "roots": [
      "./src",
      "./tests"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "./src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/mocks/file.js",
      "\\.(css|scss|sass)$": "<rootDir>/tests/mocks/style.js"
    }
  }
}
